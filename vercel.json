
{
    "version": 2,
    "name": "slingo",
    "builds": [
       { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/auth/login", "destination": "/api/auth/login" },
       { "src": "/auth/callback", "destination": "/api/auth/callback" },
       { "src": "/auth/token", "destination": "/api/auth/token" },
       { "src": "/test", "destination": "/api/test" },
       { "src": "/(.*)", "dest": "/app.js" }
    ]
 }