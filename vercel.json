
{
    "version": 2,
    "name": "slingo",
    "builds": [
       { "src": "/(.*)", "use": "@vercel/node" }
    ],
    "rewrites": [
       { "src": "/auth/login", "dest": "/api/auth/login" },
       { "src": "/auth/callback", "dest": "/api/auth/callback" },
       { "src": "/auth/token", "dest": "/api/auth/token" },
       { "src": "/test", "dest": "/api/test" },
       { "src": "/(.*)", "dest": "/" }
    ]
 }

